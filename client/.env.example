# Frontend Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:5000
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_DEBUG_MODE=true

# External Services (handled by backend, but can be used for direct calls if needed)
VITE_HEYGEN_STREAMING_URL=https://api.heygen.com/v2/streaming

# AWS Configuration (for production deployment)
VITE_AWS_REGION=us-east-1
VITE_AWS_S3_BUCKET=medcor-frontend-assets

# CDN Configuration (for production)
VITE_CDN_URL=https://cdn.medcor.ai

# Authentication Configuration
VITE_AUTH_TIMEOUT=3600000
VITE_SESSION_TIMEOUT=86400000

# UI Configuration
VITE_THEME=light
VITE_LANGUAGE=en

# Performance Configuration
VITE_LAZY_LOADING=true
VITE_CACHE_TIMEOUT=300000

# Error Handling
VITE_ERROR_RETRY_ATTEMPTS=3
VITE_ERROR_RETRY_DELAY=1000